<% puts "askdşaskdkşasd" %>
<% puts params[:bid_name] %>
<%= simple_form_for(@bid_product) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.association  :bid, :value => params[:bid_name], disabled: false %>
    <%= f.association :product%>
    <%= f.association :unit %>
    <%= f.input :quantity %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
